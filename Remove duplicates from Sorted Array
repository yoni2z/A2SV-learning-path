class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        i = 0
        
        while (i <= len(nums) - 1):
            if (len(nums) == 1):
                print(nums)
                i += 1
            else:
                if nums[i - 1] == nums[i]:
                    nums.pop(i)
                else:
                    i += 1
        n = len(nums)
        return n
